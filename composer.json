{
    "name": "mukja/posty",
    "description": "Posty Cli",
    "keywords": ["posty", "post-cli", "laravel-posty-cli", "posty-console"],
    "homepage": "https://mukja.dev",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Erth0/posty/issues",
        "source": "https://github.com/Erth0/posty"
    },
    "authors": [
        {
            "name": "Eluert Mukja",
            "email": "e.mukja@icloud.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^6.5.5",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "laminas/laminas-text": "^2.7",
        "laravel-zero/framework": "^8.0",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/yaml-front-matter": "^2.0",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["posty"]
}
